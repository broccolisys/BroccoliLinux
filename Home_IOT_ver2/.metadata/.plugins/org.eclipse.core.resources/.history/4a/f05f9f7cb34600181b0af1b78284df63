#include <stdio.h>
#include <stdlib.h>

#define MEMBER 3
#define ZERO 0
typedef struct{
	char name;
	char phone;
	int age;
}info_t;

int main(void) {
	info_t info[MEMBER];
	int i=0;

	for(i=ZERO;i<MEMBER;i++){
		printf("name : ");
		scanf("%s",&info[i].name);

		printf("age : ");
		scanf("%d",&info[i].age);
	}

	printf("\n\n");
	for(i=ZERO;i<MEMBER;i++){
		printf("name : %s\n",info[i].name);
		printf("age : %d\n",info[i].age);
	}



	return EXIT_SUCCESS;
}
